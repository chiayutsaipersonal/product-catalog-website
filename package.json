{
  "name": "product-catalog-website",
  "version": "1.0.0",
  "description": "SSR product catalog website built with Nuxt and Storyblok",
  "author": "juniorCitizen <chiayu.tsai.personal@gmail.com>",
  "private": true,
  "dependencies": {
    "aos": "^3.0.0-beta.5",
    "express": "^4.16.3",
    "nuxt": "^1.4.2",
    "storyblok-nuxt": "0.0.9",
    "vue2-google-maps": "^0.10.2",
    "vuetify": "^1.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "dotenv-safe": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.7.1",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
